pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        } 
        stage('Build') {
            steps {
                echo 'Project is getting build'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Project is getting deployed'
            }
        }
         stage('Test') {
            steps {
                echo 'Project is getting tested'
            }
        }
         stage('Release') {
            steps {
                echo 'Project is getting released'
            }
        }
    }    
}
